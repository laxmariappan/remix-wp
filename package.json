    "@tailwindcss/forms": "^0.5.1",
    "autoprefixer": "^10.4.7",
    "concurrently": "^7.2.0",
    "eslint": "^8.15.0",
    "postcss": "^8.4.13",
    "tailwindcss": "^3.0.24"
  },
  "engines": {
    "node": ">=14"
